	protected int key = 3;
	protected MyBinarySearch mbs = new MyBinarySearch();
	
	@Test
	public void testBinarySearch() {
		testBinarySearchFind();
		testBinarySearchFail();
//		assertTrue((res1 && res2));
	}
	
	public int testBinarySearchFind() {	
		return mbs.binarySearch(inputArray,/*key*/3);
	}
	
	public int testBinarySearchFail() {
		return mbs.binarySearch(inputArray,/*key*/4);
	}
}
